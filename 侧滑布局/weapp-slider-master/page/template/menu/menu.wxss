
 
/* 列表详情 */
 
.li {
  background-color: rgb(24, 48, 68);
  display: flex;
  justify-content: space-between;
  font-size: 34rpx;
  width: 67%;
  padding: 0 4%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #f1f1f1;
}

.bg-gray {
   width: 59%;
  background-color: rgba(22, 108, 119, 0.664) !important;
  border-bottom: 1rpx solid #fff !important;
  padding: 0 10%;
 
}

.li .icon-xiala, .icon-shangjiantou {
  color: #999;
  font-size: 28rpx;
}

.gray {
  color: #8e8e8e;
}

.red {
  color: #fe2e2e;
}

image  {
width: 20px;
height: 17px;
display: inline-block;
overflow: hidden;

}



